{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "dist",
  "testEnvironment": "node",
  "testRegex": ".*spec.js$",
  "collectCoverage": true,
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "outputDirectory": "../../reports/junit/server",
        "outputName": "jest.xml"
      }
    ]
  ]
}
